{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended", "tslint-react"],
  "jsRules": {},
  "rules": {
    "quotemark": [true, "single", "jsx-double"],
    "arrow-parens": false,
    "member-access": [true, "no-public"],
    "jsx-no-multiline-js": false,
    "curly": false,
    "trailing-comma": [true, { "multiline": "always", "singleline": "never" }]
  },
  "rulesDirectory": []
}
